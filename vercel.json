{
  "version": 2,
  "builds": [
    {
      "src": "client/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "api/test_js.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/*.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "server/**"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/api/test-js",
      "dest": "api/test_js.js"
    },
    {
      "src": "/api/test",
      "dest": "api/test.ts"
    },
    {
      "src": "/api/(.*)",
      "dest": "api/index.ts"
    },
    {
      "src": "/(.*\\..*)",
      "dest": "client/dist/$1"
    },
    {
      "src": "/(.*)",
      "dest": "client/dist/index.html"
    }
  ]
}